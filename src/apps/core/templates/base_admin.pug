  body.fixed-header.menu-pin(style='background-color: #f9f9f9')
    nav.page-sidebar(data-pages='sidebar')
      #appMenu.sidebar-overlay-slide.from-top
      .sidebar-header
        .col-12
          .text-center
            h2 TRYS
      .sidebar-menu
        ul.menu-items
          li.m-t-30
            a(href="{% url 'profiles:home' %}")
              span.title Dashboard
            span.icon-thumbnail
              i(data-feather='home')
          li
            a(href='javascript:;')
              span.title Tokens
              span.arrow
            span.icon-thumbnail
              i(data-feather='package')
            ul.sub-menu
              li
                a(href="{% url 'tokens:admin-token-list' %}") Listado
                span.icon-thumbnail
                  i.fa.fa-list
              li
                a(href="{% url 'tokens:admin-create-token-type' %}") AÃ±adir tipo de token
                span.icon-thumbnail
                  i.fa.fa-plus
          li
            a(href='javascript:;')
              span.title Pozos
              span.arrow
            span.icon-thumbnail
              i(data-feather='package')
            ul.sub-menu
              li
                a(href="{% url 'tokens:admin-pool-list' %}") Listado
                span.icon-thumbnail
                  i.fa.fa-list
              li
                a(href="{% url 'tokens:admin-create-pool' %}") Crear pozo
                span.icon-thumbnail
                  i.fa.fa-plus
          li
            a(href="{% url 'profiles:logout' %}")
              span.title Logout
            span.icon-thumbnail
              i(data-feather='log-out')
        .clearfix

    .page-container(style='padding-left: 0px;')
      .page-content-wrapper
        .content(style='padding-top: .5rem;')
          .container-fluid.container-fixed-lg
            nav.navbar
              a.navbar-brand.d-flex.align-items-center(href='/')
                .title
              .float-right
                i.fa.fa-user.m-r-10
                span #{user.name}
            .m-l-30
              block content
    script(src="{% static 'pages/plugins/feather-icons/feather.min.js' %}", type='text/javascript')
